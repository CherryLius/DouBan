<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Banner">
        <attr name="indicatorWidth" format="dimension" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="indicatorGap" format="dimension" />
        <attr name="indicatorPadding" format="dimension" />
        <attr name="indicatorDrawable" format="reference" />
        <attr name="numIndicatorSize" format="dimension" />
        <attr name="numIndicatorBackground" format="reference" />
        <attr name="titleHeight" format="dimension" />
        <attr name="titlePadding" format="dimension" />
        <attr name="titleBackground" format="reference" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="pagerScrollDuration" format="integer" />
        <attr name="bannerDuration" format="integer" />
        <attr name="indicatorStyle" format="enum">
            <enum name="nonIndicator" value="0" />
            <enum name="circleIndicator" value="1" />
            <enum name="numberIndicator" value="2" />
            <enum name="titleNumberIndicator" value="3" />
            <enum name="titleCircleIndicator" value="4" />
            <enum name="titleCircleIndicatorInside" value="5" />
        </attr>
        <attr name="indicatorGravity" format="enum">
            <enum name="left" value="6" />
            <enum name="center" value="7" />
            <enum name="right" value="8" />
        </attr>
    </declare-styleable>
</resources>